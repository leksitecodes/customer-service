{
  "version": 2,
  "builds": [
    {
      "src": "customer_service_bot/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "customer_service_bot/wsgi.py" }
  ]
}

  