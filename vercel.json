{
  "build": {
    "env": {
      "DJANGO_SETTINGS_MODULE": "customer_service_bot.settings.production"
    },
    "cmd": "pip install -r requirements.txt && python manage.py collectstatic --noinput"
  },
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "chat.html" }
  ]
}


  